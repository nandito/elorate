{
  "name": "elorate",
  "version": "0.0.1",
  "description": "Sort items using ELO rating",
  "main": "index.js",
  "scripts": {
    "build": "watchify src/main.js -o public/bundle.js -t [ babelify --presets [ react es2015 ] ]",
    "server": "cd public; live-server --port=1234 --entry-file=index.html",
    "lint": "eslint --cache --ext=js,jsx src",
    "test": "mocha --require babel-register --recursive",
    "test:watch": "mocha --require babel-register --recursive --watch"
  },
  "keywords": [
    "decide",
    "rate",
    "elo",
    "sort",
    "ranking",
    "decision",
    "making"
  ],
  "author": "Nandor Biro",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "live-server": "^1.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "uuid": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^2.1.1",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.1"
  }
}
